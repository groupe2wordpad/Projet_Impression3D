    {
      "src": "/static/img/icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],

  "screenshots": [
    {
      "src": "/static/img/screenshots/homepage.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide"
    },
    {
      "src": "/static/img/screenshots/homepage-mobile.png",
      "sizes": "390x844",
      "type": "image/png"
    }
  ],

  "protocol_handlers": [
    {
      "protocol": "web+nbotumbo",
      "url": "/?action=%s"
    }
  ]
